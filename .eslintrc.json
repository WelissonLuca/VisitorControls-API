{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": ["standard", "prettier"],
	"parserOptions": {
		"ecmaVersion": 12
	},
        
     "plugins": [
    "prettier"
  ],

	"rules": {

		"new-cap": 0,
		"eqeqeq": "off",
        "prettier/prettier": "error"
	}
}
